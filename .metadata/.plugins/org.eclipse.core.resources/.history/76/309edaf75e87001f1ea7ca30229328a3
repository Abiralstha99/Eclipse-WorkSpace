import java.util.*;
public class CountDown2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner (System.in);
		System.out.println("Enter the size: ");
		int n = sc.nextInt();
		for (int i = n;i>0;i--) {
			for (int j = i ; j>0;j--) {
				System.out.print(i + " "); 
			}
			System.out.println();
		}
	}

}
